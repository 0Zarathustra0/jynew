<?xml version="1.0" encoding="utf-8"?>

<bean name="jyx2skillDisplay" from="武功招式特效.xlsx" to="jyx2skillDisplay.xml" >
    <variable name="Id" fromCol="代号" type="string" />
	<variable name="SkillEffectCode" fromCol="技能动画" type="string" />
	<variable name="WeaponCode" fromCol="武器绑定" type="string" />
	<variable name="AnimationController" fromCol="动画控制器" type="string" defaultValue="Assets/BuildSource/AnimationControllers/jyx2humanoidController.controller"/>
	<variable name="PoseCode" fromCol="动作ID" type="string" defaultValue="1"/>
	<variable name="AnimaionDelay" fromCol="出招动作延迟" type="float" defaultValue="0"/>
	<variable name="AttackCode" fromCol="攻击ID" type="string" defaultValue="1"/>
	<variable name="Duration" fromCol="动画时长" type="float" defaultValue="1.5"/>
	<variable name="HitDelay" fromCol="受击延迟" type="float" defaultValue="0"/>
	<variable name="CastEft" fromCol="施展特效" type="string"/>
	<variable name="CastDelay" fromCol="施展特效延迟" type="float" defaultValue="0"/>
	<variable name="CastOffset" fromCol="施展特效偏移" type="string"/>
	<variable name="BlockEft" fromCol="格子特效" type="string"/>
	<variable name="BlockDelay" fromCol="格子特效延迟" type="float" defaultValue="0"/>
	<variable name="BlockOffset" fromCol="格子特效偏移" type="string"/>
	<variable name="AudioEft" fromCol="音效" type="string"/>
	<variable name="AudioEftDelay" fromCol="音效延迟" type="float" defaultValue="0"/>
</bean>